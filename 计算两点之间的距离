#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    double x1,x2,y1,y2;
    double a,b,m;
    while(scanf("%lf%lf%lf%lf",&x1,&x2,&y1,&y2)!=EOF)
    {
        a=(y1-x1)*(y1-x1);
        b=(y2-x2)*(y2-x2);
        m=sqrt(a+b);
        printf("%.2lf\n",m);
    }


    return 0;
}
